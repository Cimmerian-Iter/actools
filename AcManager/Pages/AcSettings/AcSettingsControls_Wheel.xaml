<UserControl x:Class="AcManager.Pages.AcSettings.AcSettingsControls_Wheel" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mui="http://firstfloorsoftware.com/ModernUI" xmlns:g="clr-namespace:AcManager"
    mc:Ignorable="d">
  <mui:ModernTab Layout="Tab" SelectedSource="/Pages/AcSettings/AcSettingsControls_Wheel_Main.xaml" SaveKey="__AcSettingsControls_Wheel.SelectedPage"
      Margin="0 0 0 0" LinksMargin="0 0 44 0" FrameMargin="0 28 0 0">
    <mui:ModernTab.Links>
      <mui:Link DisplayName="{x:Static g:AppStrings.Controls_Axis}" Source="/Pages/AcSettings/AcSettingsControls_Wheel_Main.xaml" />
      <mui:Link DisplayName="{x:Static g:AppStrings.Controls_Buttons}" Source="/Pages/AcSettings/AcSettingsControls_Wheel_Buttons.xaml" />
      <mui:Link DisplayName="{x:Static g:AppStrings.AcSettings_System}" Source="/Pages/AcSettings/AcSettingsControls_Wheel_System.xaml" />
      <mui:Link DisplayName="Patch" Source="/Pages/AcSettings/AcSettingsControls_Wheel_Patch.xaml" />
      <mui:Link DisplayName="{x:Static g:AppStrings.Controls_ForceFeedback}" Source="/Pages/AcSettings/AcSettingsControls_Wheel_ForceFeedback.xaml" />
    </mui:ModernTab.Links>
  </mui:ModernTab>
</UserControl>